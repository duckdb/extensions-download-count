{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "extensions-download-count",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-17",
	"observability": {
		"enabled": true
	},
	"bindings" : {
		"r2_buckets": [
			{
				"binding": "extension-test-bucket",
				"bucket_name": "extensions-test-bucket"
			}
		]
	},
	"triggers": {
		"crons": [
			"0 4 * * *"
		]
	}
}