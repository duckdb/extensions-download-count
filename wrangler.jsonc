{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "extensions-download-count",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-13",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "community_extensions_duckdb_org",
			"bucket_name": "duckdb-community-extensions"
		},
		{
			"binding": "extensions_duckdb_org",
			"bucket_name": "duckdb-core-extensions"
		}
	],
	"triggers": {
		"crons": [
			"0 4 * * *"
		]
	}
}